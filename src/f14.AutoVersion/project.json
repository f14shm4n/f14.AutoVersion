{
  "version": "1.0.1",
  "authors": [ "f14shm4n" ],
  "description": "Tool for auto-incrementing version of .Net Core app.",

  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "outputName": "dotnet-autover",
    "compile": {
      "exclude": [
        "*.ps1"
      ]
    }
  },

  "packOptions": {
    "owners": [ "f14shm4n" ],

    "projectUrl": "https://github.com/f14shm4n/f14.AutoVersion",
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0.html",

    "repository": {
      "type": "git",
      "url": "https://github.com/f14shm4n/f14.AutoVersion.git"
    },

    "tags": [
      "aspnetcore",
      ".netcore",
      "netcore",
      "version",
      "autoversion",
      "increment"
    ]
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },
    "Newtonsoft.Json": "9.0.1"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50"
      ]
    }
  }

}
